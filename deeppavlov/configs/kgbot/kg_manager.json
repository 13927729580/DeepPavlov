{
  "chainer": {
    "in": ["utterance"],
    "pipe": [
      {
        "name": "kg_data",
        "id": "kg_data"
      },
      {
        "name": "kg_state",
        "id": "kg_state",
        "data": "#kg_data.data",
        "in": ["utterance"],
        "out": ["utter_history", "slot_history", "last_cluster_id", "user_id"]
      },
      {
        "name": "kg_tfidf_scorer",
        "data": "#kg_data.data",
        "in": ["utterance"],
        "out": ["tfidf_scores"]
      },
      {
        "name": "kg_tagger",
        "data": "#kg_data.data",
        "threshold": 75,
        "in": ["utterance"],
        "out": ["tag_scores"]
      },
      {
        "name": "kg_tags2events_scorer",
        "data": "#kg_data.data",
        "in": ["tag_scores"],
        "out": ["tag_events_scores"]
      },
      {
        "name": "kg_filter",
        "data": "#kg_data.data",
        "n_top": 30,
        "in": ["tfidf_scores", "tag_events_scores"],
        "out": ["filtered_events"]
      },
      {
        "name": "kg_manager",
        "min_num_events": 20,
        "cluster_policy": {
          "name": "kb_cluster_policy",
          "slots": "#kb_data.data['slots']",
          "min_rate": 0.01,
          "max_rate": 0.99
        },
        "in": ["filtered_events", "slot_history", "utter_history"],
        "out": ["message", "cluster_id"]
      },
      {
        "name": "kg_state_saver",
        "states": "#kg_state.state",
        "in": ["user_id", "slot_history", "cluster_id"],
        "out": ["user_id"]
      }
    ],
    "out": ["message"]
  }
}
