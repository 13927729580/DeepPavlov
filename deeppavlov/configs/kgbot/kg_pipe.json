{
  "chainer": {
    "in": ["utterance"],
    "pipe": [
      {
        "name": "kg_data",
        "id": "kg_data"
      },
      {
        "name": "kg_tfidf_scorer",
        "data": "#kg_data.data",
        "n_last_utts": 3,
        "in": ["utterance"],
        "out": ["tfidf_scores"]
      },
      {
        "name": "kg_tagger",
        "data": "#kg_data.data",
        "threshold": 75,
        "in": ["utterance"],
        "out": ["tag_scores"]
      },
      {
        "name": "kg_tags2events_scorer",
        "data": "#kg_data.data",
        "in": ["tag_scores"],
        "out": ["tag_events_scores"]
      },
      {
        "name": "kg_filter",
        "data": "#kg_data.data",
        "n_top": 3,
        "in": ["tfidf_scores", "tag_events_scores"],
        "out": ["message"]
      }
    ],
    "out": ["message"]
  }
}