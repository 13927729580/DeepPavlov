{
  "chainer": {
    "in": ["utterance"],
    "pipe": [
      {
        "name": "kg_data",
        "id": "kg_data"
      },
      {
        "name": "kg_ranker",
        "data": "#kg_data.data",
        "data_type": "events",
        "n_top": -1,
        "in": ["utterance"],
        "out": ["id", "score"]
      },
      {
        "name": "kg_tagger",
        "data": "#kg_data.data",
        "data_type": "events",
        "threshold": 75,
        "in": ["utterance"],
        "out": ["tag_scores"]
      },
      {
        "name": "kg_filter",
        "data": "#kg_data.data",
        "data_type": "events",
        "n_top": 3,
        "in": ["id", "score", "tag_scores"],
        "out": ["message"]
      }
    ],
    "out": ["message"]
  },
  "train": {
    "show_examples": false,
    "test_best": false,
    "validate_best": true,
    "log_every_n_batches": 250,
    "val_every_n_epochs": 1,
    "batch_size": 50,
    "pytest_max_batches": 2,
    "metrics": ["squad_f1", "exact_match"]
  }
}