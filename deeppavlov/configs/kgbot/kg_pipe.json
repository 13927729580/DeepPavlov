{
  "chainer": {
    "in": ["utterance"],
    "pipe": [
      {
        "name": "kg_data",
        "id": "kg_data"
      },
      {
        "name": "kg_ranker",
        "data": "#kg_data.data",
        "n_top": -1,
        "in": ["utterance"],
        "out": ["id", "score"]
      },
      {
        "name": "kg_tagger",
        "data": "#kg_data.data",
        "threshold": 75,
        "in": ["utterance"],
        "out": ["tag_scores"]
      },
      {
        "name": "kg_filter",
        "data": "#kg_data.data",
        "n_top": 3,
        "in": ["id", "score", "tag_scores"],
        "out": ["message"]
      }
    ],
    "out": ["message"]
  }
}