{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "in_y": [
      "ans_raw",
      "ans_raw_start"
    ],
    "out": [
      "ans_predicted"
    ],
    "pipe": [
      {
        "name": "tfidf_ranker",
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids",
          "doc_scores"
        ],
        "fit_on_batch": [
          "questions_raw"
        ],
        "vectorizer": {
          "name": "hashing_tfidf_vectorizer",
          "fit_on_batch": [
            "questions_raw"
          ],
          "save_path": "odqa/ruwiki_tfidf_matrix.npz",
          "load_path": "odqa/ruwiki_tfidf_matrix.npz",
          "tokenizer": {
            "name": "ru_tokenizer",
            "lemmas": true,
            "ngram_range": [
              1,
              2
            ]
          }
        }
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "context_raw"
        ],
        "data_dir": "odqa",
        "shuffle": false,
        "data_url": "http://lnsigo.mipt.ru/export/datasets/wikipedia/ruwiki.db"
      },
      {
        "config_path": "../deeppavlov/configs/squad/squad_ru.json",
        "in": [
          "context_raw",
          "question_raw"
        ],
        "out": [
          "ans_predicted",
          "ans_start_predicted"
        ]
      }
    ]
  },
  "metadata": {
    "labels": {
      "server_utils": "ODQA"
    },
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/ru_odqa.tar.gz",
      "http://lnsigo.mipt.ru/export/deeppavlov_data/squad_model_ru.tar.gz"
    ]
  }
}