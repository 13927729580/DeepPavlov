{
  "dataset_reader": {
    "name": "odqa_reader",
    "dataset_format": "txt",
    "save_path": "",
    "data_path": ""
  },
  "dataset_iterator": {
    "name": "sqlite_iterator",
    "save_path": "",
    "shuffle": false,
    "load_path": ""
  },
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "context_raw"
    ],
    "pipe": [
      {
        "name": "tfidf_ranker",
        "top_n": 1,
        "active": true,
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids",
          "doc_scores"
        ],
        "fit_on_batch": [
          "questions_raw"
        ],
        "vectorizer": {
          "name": "hashing_tfidf_vectorizer",
          "fit_on_batch": [
            "questions_raw"
          ],
          "save_path": "",
          "load_path": "",
          "tokenizer": {
            "name": "ru_tokenizer",
            "lemmas": true,
            "ngram_range": [
              1,
              2
            ]
          }
        }
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "contexts_as_docs"
        ],
        "save_path": "",
        "shuffle": false,
        "join_docs": false,
        "load_path": ""
      },
      {
        "name": "sentence_splitter",
        "in": [
          "contexts_as_docs"
        ],
        "out": [
          "contexts_as_sentences"
        ]
      },
      {
        "name": "query_paragraph_tuplifier",
        "in": [
          "question_raw",
          "contexts_as_sentences"
        ],
        "out": [
          "question_contexts_pairs"
        ]
      },
      {
        "name": "sentence_ranker",
        "top_n": 1,
        "active": true,
        "in": [
          "question_contexts_pairs"
        ],
        "out": [
          "context_raw"
        ]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 10000
  },
  "metadata": {
    "download": [
    ]
  }
}