{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "context_raw"
    ],
    "pipe": [
      {
        "name": "sqlite_index",
        "in": ["question_raw"],
        "out": [
          "doc_ids"
        ],
        "data_dir": "pmef",
        "data_url": "http://lnsigo.mipt.ru/export/datasets/pmef/en_drones.db"
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": ["doc_ids"],
        "out": [
          "contexts_as_docs"
        ],
        "data_dir": "pmef",
        "shuffle": false,
        "join_docs": false,
        "data_url": "http://lnsigo.mipt.ru/export/datasets/pmef/en_drones.db"
      },
      {
        "name": "sentence_splitter",
        "in": [
          "contexts_as_docs"
        ],
        "out": [
          "contexts_as_sentences"
        ]
      },
      {
        "name": "query_paragraph_tuplifier",
        "in": [
          "question_raw",
          "contexts_as_sentences"
        ],
        "out": [
          "question_contexts_pairs"
        ]
      },
      {
        "name": "sentence_ranker",
        "top_k": 1000,
        "return_vectors": true,
        "in": [
          "question_contexts_pairs"
        ],
        "out": [
          "context_raw"
        ]
      }
    ]
  },
  "metadata": {
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/pmef.tar.gz"
    ]
  }
}