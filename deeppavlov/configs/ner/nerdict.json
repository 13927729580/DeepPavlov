{
  "chainer": {
    "in": ["x"],
    "pipe": [
      {
        "in": ["x"],
        "name": "lazy_tokenizer",
        "out": ["x_toks"]
      },
      {
        "in": ["x_toks"],
        "name": "pymorphy_russian_lemmatizer",
        "out":["x_lemma"]
      },
      {
        "in": ["x_lemma"],
        "name": "nerdict",
        "save_path": "world_cities_on_rus.csv",
        "load_path": "world_cities_on_rus.csv",
        "out": ["tags"]
      }
    ],
    "out": ["tags"]
  },
  "metadata": {
    "labels": {
      "telegram_utils": "NERCoNLL2003Model",
      "server_utils": "NER"
    },
    "download": [
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/lenta_lower_100.bin",
        "subdir": "embeddings"
      }
    ]
  }
}

